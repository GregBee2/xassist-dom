{
  "name": "@xassist/xassist-dom",
  "version": "1.0.0",
  "description": "Basic helperfunctions for DOM-elements",
  "keywords": [
    "classoperations",
    "getDimensions",
    "DOM"
  ],
  "bugs": {
    "email": "gregory.beirens@gmail.com"
